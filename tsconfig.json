{
  "compilerOptions": {
    "rootDir": "lib",
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./Pack",
    "moduleResolution": "node"
  },
  "lib": [
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "dom"
  ],
  "include": [ "lib/**/*" ]
}

